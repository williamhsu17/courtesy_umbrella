<h1>Station:<%= @station.name %></h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>進站時間</th>
    </tr>
  </thead>
  <tbody>
    <% @umbrellas.each do |umbrella| %>
      <tr>
        <td><%= umbrella.id %></td>
        <td><%= umbrella.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

  <%= form_tag return_station_index_path(:location_id => params[:id]), :method => :post do %>
    <%= number_field_tag :id %>
    <%= submit_tag "歸還雨傘" %>
  <% end %>